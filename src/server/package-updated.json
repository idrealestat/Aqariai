{
  "name": "waseety-backend",
  "version": "1.0.0",
  "description": "Waseety Real Estate CRM Backend API with Real-time & Workers",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "worker": "node worker.js",
    "realtime": "node realtime-server.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run worker\" \"npm run realtime\"",
    "db:setup": "psql -f schema.sql",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "docs": "swagger-jsdoc -d swaggerDef.js -o ./docs/swagger.json ./routes/*.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.0",
    "compression": "^1.7.4",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "pg": "^8.11.3",
    "ioredis": "^5.3.2",
    "bullmq": "^4.15.0",
    "socket.io": "^4.7.2",
    "joi": "^17.11.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "axios": "^1.6.0",
    "nodemailer": "^6.9.7",
    "winston": "^3.11.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.54.0"
  },
  "keywords": [
    "real-estate", 
    "crm", 
    "api", 
    "backend", 
    "nodejs", 
    "express", 
    "websocket", 
    "realtime", 
    "workers"
  ],
  "author": "Waseety Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}